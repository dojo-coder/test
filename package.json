{
  "name": "test",
  "msw": {
    "workerDirectory": "test"
  },
  "description": "Setup for Mocha unit testing",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js --progress",
    "update": "npx msw init ./ --no-save",
    "build": "webpack --config config/webpack.prod.js",
    "prod": "http-server ./dist -a localhost -p 8000 -o",
    "test:watch": "cross-env REPORTER=mocha karma start --browsers ChromeHeadless karma.conf.js",
    "test": "cross-env COVERAGE=true karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:update": "cross-env COVERAGE=true UPDATE=1 karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:ci": "cross-env REPORTER=html COVERAGE=true karma start --no-fail-on-failing-test-suite --single-run --browsers ChromeHeadless karma.conf.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-arrow-functions": "^7.12.1",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.1",
    "@pinia/testing": "^0.0.12",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-karma-snapshot": "^0.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^5.0.0-alpha.7",
